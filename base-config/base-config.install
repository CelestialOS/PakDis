post_install() {
	bin/svmgr enable getty-tty1 quiet
	bin/svmgr enable getty-tty2 quiet
	bin/svmgr enable getty-tty3 quiet
	bin/svmgr enable getty-tty4 quiet
}

pre_remove() {
	bin/svmgr disable getty-tty1 quiet
	bin/svmgr disable getty-tty2 quiet
	bin/svmgr disable getty-tty3 quiet
	bin/svmgr disable getty-tty4 quiet
}
